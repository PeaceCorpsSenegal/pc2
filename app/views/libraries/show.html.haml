.container
  .row
    .span-5
      = render 'shared/menu'
    .span-13
      %h1= @library.name
      %h4 Photos
      - @library.stacks.each do |s|
        = render s.stackable
    .span-6.last
      %h4= @library.description
      %p= @library.country
      %p This library is managed by
      = render @library.user
